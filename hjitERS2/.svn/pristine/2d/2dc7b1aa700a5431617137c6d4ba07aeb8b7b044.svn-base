package kr.co.hjitERS.vo;

public class LoginVO {
	private String cu_gubun;
	private String cu_adcode;
	private String cu_passwd;
	
	public String getCu_gubun() {
		return cu_gubun;
	}
	public void setCu_gubun(String cu_gubun) {
		this.cu_gubun = cu_gubun;
	}
	public String getCu_adcode() {
		return cu_adcode;
	}
	public void setCu_adcode(String cu_adcode) {
		this.cu_adcode = cu_adcode;
	}
	public String getCu_passwd() {
		return cu_passwd;
	}
	public void setCu_passwd(String cu_passwd) {
		this.cu_passwd = cu_passwd;
	}
	public LoginVO(String cu_gubun, String cu_adcode, String cu_passwd) {
		super();
		this.cu_gubun = cu_gubun;
		this.cu_adcode = cu_adcode;
		this.cu_passwd = cu_passwd;
	}
	public LoginVO() {
		super();
		// TODO Auto-generated constructor stub
	}
	@Override
	public String toString() {
		return "LoginVO [cu_gubun=" + cu_gubun + ", cu_adcode=" + cu_adcode + ", cu_passwd=" + cu_passwd + "]";
	}
	
	
	
}
